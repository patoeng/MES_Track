
@model IEnumerable<MES.Mvc.Models.WorkOrderProduct>

@{
    ViewBag.Title = "StatesDetails";
}

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-success">
            <div class="panel-heading">
                <h5>Product List</h5>
            </div>
            <div class="panel-body">
                <table class="table table-striped table-hover">
                    <tr>
                        <th>
                            No.
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DataMetrix)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.WorkOrderNumber)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DateTime)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Machine)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.State)
                        </th>

                    </tr>
                   @{
                       int i = 1;
                       foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @i.ToString()
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DataMetrix)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.WorkOrderNumber)
                                </td>
                               
                                <td>
                                    @Html.DisplayFor(modelItem => item.DateTime)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Machine)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.State)
                                </td>
                            </tr>
                            i++;
                        }
                   }
                </table>
            </div>
        </div>
    </div>
</div>